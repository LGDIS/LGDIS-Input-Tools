  archive_path: "/home/mrsuser/LGDIS-Input-Tools/MRS/archives/socket/jalert"
  channel_id: "JAL"

 JAL-Volcano-Normal:
  port: 36030
  archive_path: "/home/mrsuser/LGDIS-Input-Tools/MRS/archives/socket/jalert"
  channel_id: "JAL"

 JAL-Volcano-Training:
  port: 36031
  archive_path: "/home/mrsuser/LGDIS-Input-Tools/MRS/archives/socket/jalert"
  channel_id: "JAL"

 JAL-Meteorological-Normal:
  port: 36040
  archive_path: "/home/mrsuser/LGDIS-Input-Tools/MRS/archives/socket/jalert"
  channel_id: "JAL"

 JAL-Meteorological-Training:
  port: 36041
  archive_path: "/home/mrsuser/LGDIS-Input-Tools/MRS/archives/socket/jalert"
