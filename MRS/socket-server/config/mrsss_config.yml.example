# BCHのチェックサム値によるチェック要否
# true チェック要
# false チェック不要
need_checksum: false

# アプリケーションの動作モード
# 0 通常
# 1 訓練
# 2 試験
mode: 0

# Resqueの使用有無
# true 使用
# false 使用しない
use_queue: true

# 訓練モード時のRedmineプロジェクトID
trainingmode_project_id: "I04202000000000000002"

# 試験モード時のRedmineプロジェクトID
testmode_project_id: "I04202000000000000003"

