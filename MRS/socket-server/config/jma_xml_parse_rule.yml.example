# ------------------------------------------------------------
# プロジェクト自動送信用の設定
# 該当の配備番号の場合はプロジェクト自動送信扱いとする
# ------------------------------------------------------------
autosend_disposition_numbers: ["0", "1", "2", "3","4"]

# 自動送信時にissue拡張フィールドに付加する情報
autosend_extras: 
 # 発信対象地域
 "delivered_area": "042021"

 # 情報の更新種別
 "type_update": "1"

# ------------------------------------------------------------
# チケットの"説明文"に登録する内容
#
# ------------------------------------------------------------
# 
description_type_path: "/Report/Control/Title/text()"
description_target_param: "description"
description_contents: 
 "震度速報": 
  - path: "/Report/Body/Intensity/Observation/Pref/Area/Name[text()=\"宮城県中部\"]/text()"
  - 
   path: "/Report/Body/Intensity/Observation/Pref/Area[Name/text()=\"宮城県中部\"]/MaxInt/text()"
   decorator: "add_modifier"
  - path: "/Report/Body/Comments/ForecastComment/Text/text()"
 "震源に関する情報": 
  - 
   path: "/Report/Body/Earthquake/Hypocenter/Area/Name/text()"
   header: "震源位置："
  - path: "/Report/Body/Earthquake/Hypocenter/Area/Coordinate/@description"
  - path: "/Report/Body/Earthquake/Magnitude/@description"
  - path: "/Report/Body/Comments/ForecastComment/Text/text()"
 "震源・震度に関する情報": 
  - path: "/Report/Body/Intensity/Observation/Pref/Area/City/Name[text()=\"石巻市\"]/text()"
  - 
   path: "/Report/Body/Intensity/Observation/Pref/Area/City[Name/text()=\"石巻市\"]/MaxInt/text()"
   decorator: "add_modifier"
  - 
   path: "/Report/Body/Earthquake/Hypocenter/Area/Name/text()"
   header: "震源位置:"
  - path: "/Report/Body/Earthquake/Hypocenter/Area/Coordinate/@description"
  - path: "/Report/Body/Earthquake/Magnitude/@description"
  - path: "/Report/Body/Comments/ForecastComment/Text/text()"
 "津波警報・注意報・予報": 
  - path: "/Report/Body/Tsunami/Forecast/Item/Area/Name[text()=\"宮城県\"]/text()"
  - path: "/Report/Body/Tsunami/Forecast/Item[Area/Name/text()=\"宮城県\"]/Category/Kind/Name/text()"
  -
   path: "/Report/Body/Tsunami/Forecast/Item[Area/Name/text()=\"宮城県\"]/FirstHeight/ArrivalTime/text()"
   header: "津波の到達予想時刻："
  - path: "/Report/Body/Tsunami/Forecast/Item[Area/Name/text()=\"宮城県\"]/FirstHeight/Condition/text()"
  - 
   path: "/Report/Body/Tsunami/Forecast/Item[Area/Name/text()=\"宮城県\"]/MaxHeight/TsunamiHeight/@description"
   header: "予想される津波の高さ："
  - path: "/Report/Body/Earthquake/Hypocenter/Area/Name/text()"
  - path: "/Report/Body/Earthquake/Hypocenter/Area/Coordinate/@description"
  - path: "/Report/Body/Earthquake/Magnitude/@description"
  - path: "/Report/Body/Comments/WarningComment/Text/text()"
 "津波情報":
  - path: "/Report/Body/Tsunami/Observation/Item/Station/Name[text()=\"石巻市鮎川\"]/text()"
  -
   path: "/Report/Body/Tsunami/Observation/Item[Station/Name/text()=\"石巻市鮎川\"]/Station/FirstHeight/ArrivalTime/text()"
   header: "津波の第１波観測時刻："
  -
   path: "/Report/Body/Tsunami/Observation/Item[Station/Name/text()=\"石巻市鮎川\"]/Station/FirstHeight/TsunamiHeight/@Type"
   delimiter: " "
  - path: "/Report/Body/Tsunami/Observation/Item[Station/Name/text()=\"石巻市鮎川\"]/Station/MaxHeight/TsunamiHeight/@description"
  -
   path: "/Report/Body/Tsunami/Observation/Item[Station/Name/text()=\"石巻市鮎川\"]/Station/MaxHeight/DateTime/text()"
   header: "津波の最大波観測時刻："
  -
   path: "/Report/Body/Tsunami/Observation/Item[Station/Name/text()=\"石巻市鮎川\"]/FirstHeight/TsunamiHeight/@Type"
   delimiter: " "
  - path: "/Report/Body/Tsunami/Observation/Item[Station/Name/text()=\"石巻市鮎川\"]/FirstHeight/TsunamiHeight/@description"
  - path: "/Report/Body/Tsunami/Forecast/Item/Station/Name[text()=\"石巻市鮎川\"]/text()"
  -
   path: "/Report/Body/Tsunami/Forecast/Item/Station[Name/text()=\"石巻市鮎川\"]/HighTideDateTime/text()"
   header: "満潮時刻："
  -
   path: "/Report/Body/Tsunami/Forecast/Item/Station[Name/text()=\"石巻市鮎川\"]/FirstHeight/ArrivalTime/text()"
   header: "津波の到達予想時刻："
  - path: "/Report/Body/Tsunami/Forecast/Item/Station[Name/text()=\"石巻市鮎川\"]/FirstHeight/Condition/text()"
  - path: "/Report/Body/Earthquake/Hypocenter/Area/Name/text()"
  - path: "/Report/Body/Earthquake/Hypocenter/Area/Coordinate/@description"
  - path: "/Report/Body/Earthquake/Magnitude/@description"
  - path: "/Report/Body/Comments/WarningComment/Text/text()"
