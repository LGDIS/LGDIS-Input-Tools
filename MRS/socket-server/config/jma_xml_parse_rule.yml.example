# ------------------------------------------------------------
# 自動配信用メッセージの設定（タイトル/見出し・要約文）
# parameterはRestI/F用のパラメータ名
# max_lengthは最大文字数(バイト長ではない)
# pathsは文言を取得するためのXPath
# 複数のXPathで値が取得できた場合は全て連結する
# ------------------------------------------------------------
send_message: 
 - 
  parameter: "mail_subject"
  max_length: 255
  paths: 
   - "/Report/Head/Title/text()"
 - 
  parameter: "summary"
  max_length: 2000
  paths:
   - "/Report/Control/PublishingOffice/text()"
   - "/Report/Head/InfoType/text()"
   - "/Report/Head/Headline/Text/text()"
   - "/Report/Body/Warning/Item/Area/Name[text()=\"石巻市\"]/text()"
   - "/Report/Body/Warning/Item[Area/Name/text()=\"石巻市\"]/Kind/Name[contains(text(), \"大雨\")]/text()"
   - "/Report/Body/Warning/Item[Area/Name/text()=\"石巻市\"]/Kind[contains(Name/text(), \"大雨\")]/Status/text()"
   - "/Report/Body/Warning/Item[Area/Name/text()=\"石巻市\"]/Kind/Name[contains(text(), \"洪水\")]/text()"
   - "/Report/Body/Warning/Item[Area/Name/text()=\"石巻市\"]/Kind[contains(Name/text(), \"洪水\")]/Status/text()"
   - "/Report/Body/Warning/Item[Area/Name/text()=\"石巻市\"]/Kind/Name[contains(text(), \"暴風\")]/text()"
